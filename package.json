{
  "name": "@aloreljs/bound-decorator",
  "version": "2.0.0",
  "description": "A TypeScript/ES7 decorator for automatically binding methods to the class instance",
  "keywords": [
    "bound",
    "decorator",
    "typescript",
    "babel",
    "es7",
    "bind",
    "method",
    "auto"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test:babel:legacy": "babel --config-file ./.babelrc.legacy -o test/Test.legacy.js test/Test.js",
    "test:babel:new": "babel --config-file ./.babelrc.new -o test/Test.new.js test/Test.js",
    "test:compile": "concurrently -n tsc:test \"tsc -p ./tsconfig.test.json\" \"npm:test:babel:*\"",
    "pretest": "concurrently -n build:src,build:test,clean \"alo build --targets cjs --skip-package-fields\" \"npm run test:compile && node pretest-postprocess.js\" \"rimraf coverage\"",
    "doctoc": "doctoc --github README.md",
    "test": "nyc mocha -r source-map-support/register --check-leaks --exit --full-trace test/Test.legacy.js test/Test.new.js test/Test.typescript.js",
    "test:watch": "npm --scripts-prepend-node-path=auto run test -- --watch",
    "tslint": "alo tslint -p ./tsconfig.lint.json",
    "tslint:fix": "npm --scripts-prepend-node-path=auto run tslint -- --fix",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "npm --scripts-prepend-node-path=auto run typecheck -- --watch",
    "build": "alo build && tsc --target esnext"
  },
  "repository": "git@github.com:Alorel/bound-decorator.git",
  "dependencies": {
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@alorel-personal/build-tools": "4.7.19",
    "@alorel-personal/conventional-changelog-alorel": "2.1.3",
    "@alorel-personal/semantic-release": "1.4.0",
    "@alorel-personal/tslint-rules": "5.0.7",
    "@babel/cli": "~7.16.7",
    "@babel/core": "~7.16.7",
    "@babel/plugin-proposal-class-properties": "~7.16.7",
    "@babel/plugin-proposal-decorators": "~7.16.7",
    "@babel/preset-env": "~7.16.7",
    "@semantic-release/changelog": "~5.0.1",
    "@semantic-release/exec": "~3.3.8",
    "@semantic-release/git": "~7.0.18",
    "@types/chai": "~4.3.0",
    "@types/lodash": "^4.14.178",
    "@types/mocha": "~5.2.6",
    "@types/node": "~12.12.70",
    "chai": "~4.3.4",
    "concurrently": "~5.0.2",
    "coveralls": "~3.0.14",
    "doctoc": "~1.4.0",
    "lodash": "^4.17.21",
    "mocha": "~6.2.3",
    "nyc": "~15.1.0",
    "rimraf": "~3.0.2",
    "semantic-release": "~15.13.32",
    "source-map-support": "~0.5.21",
    "ts-node": "~8.5.4",
    "typescript": "~4.5.4"
  },
  "license": "MIT",
  "main": "index.js",
  "browser": "_bundle/umd.js",
  "jsdelivr": "_bundle/umd.min.js",
  "fesm5": "_bundle/fesm5.js",
  "esm5": "_bundle/fesm5.js",
  "fesm2015": "_bundle/fesm2015.js",
  "esm2015": "_bundle/fesm5.js",
  "types": "index.d.ts",
  "module": "_bundle/fesm5.js",
  "typings": "index.d.ts"
}
